<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">
    <!--width设置虚线宽度-->
    <!--dashGap设置虚线空隙宽度-->
    <!--dashWidth设置虚线的实线长度-->
    <!--color设置虚线颜色-->
